            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="wifiPosition"
            />
        <Button
            android:id="@+id/copyAsset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="copyAsset"
            />
        <Button
            android:id="@+id/pullListViewSelf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
